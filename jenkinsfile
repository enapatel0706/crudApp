node{
    
    stage('Git')
    {
        git 'https://github.com/enapatel0706/crudApp.git'
    }
    stage('Build'){
      
       "npm install"
       "npm run build"
            
        
    }
    stage('Deploye'){
        "npm run deploye"
    }
    stage('Test'){
        "node test"
    }
    
}
